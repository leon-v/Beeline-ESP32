{
    "name" : "MQTT Client",
    "description": "Provides an MQTT conenction for message routing.",
    "settings": [
		{
			"name":"host",
			"label": "Host Name",
			"default": "mqtt.example.com"
		},
		{
			"name":"port",
			"label": "Port",
			"default": 1883
		},
		{
			"name":"clientId",
			"label": "Client ID",
			"default": "Beeline_[UID]"
		},
		{
			"name":"username",
			"label": "Username",
			"default": ""
		},
		{
			"name":"password",
			"label": "Password",
			"default": ""
		},
		{
			"name":"subTopic",
			"label": "Subscribe Topic",
			"default": "/beeline/sink/#"
		},
		{
			"name":"subQos",
			"label": "Subscribe QOS",
			"default": 1,
			"options":{
				"0":"At Most Once",
				"1":"At Least Once",
				"2":"Exactly Once"
			}
		},
		{
			"name":"pubTopic",
			"label": "Publish Topic",
			"default": "/beeline/source"
		},
		{
			"name":"pubQos",
			"label": "Publish QOS",
			"default": 1,
			"options":{
				"0":"At Most Once",
				"1":"At Least Once",
				"2":"Exactly Once"
			}
		}
	]
}